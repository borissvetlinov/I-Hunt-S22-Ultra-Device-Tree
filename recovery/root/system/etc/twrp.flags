# Android fstab file.
# The filesystem that contains the filesystem checker binary (typically /system) cannot
# specify MF_CHECK, and must come before any filesystems that do specify MF_CHECK

# mount point  fstype  device                             device2                flags

/boot               emmc    /dev/block/bootdevice/by-name/boot                   flags=display="Boot Image";backup=1;flashimg=1
/cache              ext4    /dev/block/bootdevice/by-name/cache                  flags=display="Cache";wipeingui;backup=0
/recovery           emmc    /dev/block/bootdevice/by-name/recovery               flags=display="Recovery Image";backup=1;flashimg=1

/metadata           ext4    /dev/block/bootdevice/by-name/md_udc               flags=display="Metadata";backup=0;mounttodecrypt;wrappedkey

/frp               emmc    /dev/block/bootdevice/by-name/persistent                   flags=backup=1;display="FRP IMAGE";backup=1;flashimg=1
/nvram               emmc    /dev/block/bootdevice/by-name/nvram                   flags=backup=1;display="NVRAM Image";backup=1;flashimg=1
/proinfo               emmc    /dev/block/bootdevice/by-name/proinfo                   flags=backup=1;display="Proinfo Image";backup=1;flashimg=1
/bootloader               emmc    /dev/block/bootdevice/by-name/lk                   flags=backup=1;display="LK Image";backup=1;flashimg=1
/bootloader2               emmc    /dev/block/bootdevice/by-name/lk2                   flags=backup=1;display="LK2 Image";backup=1;flashimg=1
/para               emmc    /dev/block/bootdevice/by-name/misc                   flags=backup=1;display="PARA Image";backup=1;flashimg=1

# AVB Partition
/dtbo               emmc    /dev/block/bootdevice/by-name/dtbo                   flags=backup=1;display="DTBo Image";backup=1;flashimg=1
/vbmeta             emmc    /dev/block/bootdevice/by-name/vbmeta                 flags=display="VBMeta Image";backup=1;flashimg=1
/vbmeta_system      emmc    /dev/block/bootdevice/by-name/vbmeta_system		     flags=display="VBMeta System Image";backup=1;flashimg=1
/vbmeta_vendor      emmc    /dev/block/bootdevice/by-name/vbmeta_vendor		     flags=display="VBMeta Vendor Image";backup=1;flashimg=1

/logo emmc /dev/block/platform/bootdevice/by-name/logo flags=backup=1;display=logo
/expdb emmc /dev/block/platform/bootdevice/by-name/expdb flags=backup=1;display=expdb
/seccfg emmc /dev/block/platform/bootdevice/by-name/seccfg flags=backup=1;display=seccfg

/tee1 emmc /dev/block/platform/bootdevice/by-name/tee1 flags=backup=1;display=tee1
/tee2 emmc /dev/block/platform/bootdevice/by-name/tee2 flags=backup=1;display=tee2

/scp1 emmc /dev/block/platform/bootdevice/by-name/scp1 flags=backup=1;display=scp1
/scp2 emmc /dev/block/platform/bootdevice/by-name/scp2 flags=backup=1;display=scp2

/sspm_1 emmc /dev/block/platform/bootdevice/by-name/sspm_1 flags=backup=1;display=sspm1
/sspm_2 emmc /dev/block/platform/bootdevice/by-name/sspm_2 flags=backup=1;display=sspm2

/md1img emmc /dev/block/platform/bootdevice/by-name/md1img flags=backup=1;display=md1img
/md1dsp emmc /dev/block/platform/bootdevice/by-name/md1dsp flags=backup=1;display=md1dsp
/md1arm7 emmc /dev/block/platform/bootdevice/by-name/md1arm7 flags=backup=1;display=md1arm7
/md3img emmc /dev/block/platform/bootdevice/by-name/md3img flags=backup=1;display=md3img

/gz1 emmc /dev/block/platform/bootdevice/by-name/gz1 flags=backup=1;display=gz1
/gz2 emmc /dev/block/platform/bootdevice/by-name/gz2 flags=backup=1;display=gz2

/spmfw emmc /dev/block/platform/bootdevice/by-name/spmfw flags=backup=1;display=spmfw

/boot_para emmc /dev/block/platform/bootdevice/by-name/boot_para flags=backup=1;display="Boot Para"

/dtbo1 emmc /dev/block/platform/bootdevice/by-name/dtbo1 flags=backup=1;subpartitionof=/dtbo
/dtbo2 emmc /dev/block/platform/bootdevice/by-name/dtbo2 flags=backup=1;subpartitionof=/dtbo

# Persistent Firmware & Properties
/persist            ext4    /dev/block/bootdevice/by-name/persist                flags=display="Persist"

# Removable storage
/usb_otg            auto    /dev/block/sdg1        /dev/block/sdg                flags=fsflags=utf8;display="USB-OTG";storage;wipeingui;removable
/external_sd        auto     /dev/block/mmcblk0p1  /dev/block/mmcblk0            flags=display="Micro-SD Card";storage;wipeingui;removable
